{
  "name": "@wishufree/server",
  "version": "1.0.0",
  "description": "后台服务monorepo",
  "scripts": {
    "dev": "sh scripts/dev.sh",
    "build": "sh scripts/build.sh",
    "db:sync": "pnpm  --filter \"@wishufree/*\" db:sync",
    "lint:fix": "pnpm --filter \"@wishufree/*\" lint:fix",
    "lint": "pnpm --filter \"@wishufree/*\" lint",
    "prettier": "pnpm --filter \"@wishufree/*\" prettier",
    "pre-commit": "pnpm lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint",
    "**/*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@prisma/client": "^3.15.2",
    "cookie": "^0.5.0",
    "crypto-js": "^4.1.1",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "log4js": "^6.5.2",
    "moment": "^2.29.3",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/crypto-js": "^4.1.1",
    "@types/koa": "^2.13.4",
    "@types/koa-mount": "^4.0.1",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/koa__cors": "^3.3.0",
    "@types/ramda": "^0.28.23",
    "husky": "^9.0.10",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.16",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "prettier-plugin-classify-imports": "^0.0.2",
    "prisma": "^3.15.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
